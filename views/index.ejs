<% include _header %>

<h2>Welcome to Learn About Me</h2>

<% users.forEach(function(user){ %>
	<div class="panel panel-default">
		<div class="panel-heading">
			<a href="/users/<%= user.username %>"><%= user.name() %></a>	
		</div> <!-- /panel-heading -->	
		<% if (user.bio) { %>
			<div class="panel-body">
				<%= user.bio %>	
			</div> <!-- /panel-body -->
		<% } %>
	</div> <!-- /panel -->
<% }) %>

<% include _footer %>
